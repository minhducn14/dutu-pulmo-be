    "@payos/node": "^2.0.4",
    "@signpdf/placeholder-plain": "^3.3.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cloudinary": "^2.9.0",
